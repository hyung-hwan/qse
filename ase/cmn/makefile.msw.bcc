OUT = asecmn

C_SRCS = mem.c str.c
C_OBJS = $(C_SRCS:.c=.obj)

CC = bcc32
LD = ilink32
AR = tlib
JAVAC = javac

CFLAGS = -O2 -WM -WU -RT- -w -q -I../.. -DNDEBUG
LDFLAGS = -Tpd -ap -Gn -c -q 
STARTUP = c0d32w.obj
LIBS = import32.lib cw32mt.lib 

all: lib

lib: $(C_OBJS)
	$(AR) $(OUT).lib @&&!
+-$(**: = &^
+-)
!

clean:
	-del $(OBJS) $(OUT).lib *.obj 

.SUFFIXES: .c .obj 
.c.obj:
	$(CC) $(CFLAGS) -c $<
