CC = bcc32
LD = ilink32
CFLAGS = -I..\..\.. -I$(XPKIT)
LDFLAGS = -L..\..\lsp
LIBS = import32.lib cw32mt.lib aselsp.lib 
STARTUP = c0x32w.obj

all: aselsp 

aselsp: lsp.obj
	$(LD) $(LDFLAGS) $(STARTUP) lsp.obj,$@.exe,,$(LIBS),,

clean:
	del $(OBJS) *.obj $(OUT)

.SUFFIXES: .c .obj
.c.obj:
	$(CC) $(CFLAGS) -c $< 

