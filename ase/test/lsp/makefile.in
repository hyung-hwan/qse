CC = @CC@
CFLAGS = @CFLAGS@ -I@abs_top_builddir@/.. 
LDFLAGS = @LDFLAGS@ -L@abs_top_builddir@/@BUILDMODE@
LIBS = @LIBS@ -laselsp -lasecmn -laseutl -lm
MODE = @BUILDMODE@

OUT_DIR = ../../$(MODE)

all: $(OUT_DIR)/aselsp 

$(OUT_DIR)/aselsp:
	$(CC) $(CFLAGS) -o $@ lsp.c $(LDFLAGS) $(LIBS)

clean:
	rm -rf $(OUT_DIR)/aselsp 

