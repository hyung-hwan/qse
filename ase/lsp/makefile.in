SRCS = name.c token.c array.c prim.c mem.c env.c init.c read.c eval.c print.c
OBJS = $(SRCS:.c=.o)
OUT = libxplisp.a

CC = @CC@
RANLIB = @RANLIB@
CFLAGS = @CFLAGS@ -I@abs_top_builddir@
LDFLAGS = @LDFLAGS@
LIBS = @LIBS@

all: $(OBJS)
	ar cr $(OUT) $(OBJS)
	ranlib $(OUT)

clean:
	rm -rf $(OBJS) $(OUT) *.o

.SUFFIXES: .c .o
.c.o:
	$(CC) $(CFLAGS) -c $<
