SRCS = lsp.c name.c mem.c env.c err.c read.c eval.c print.c misc.c \
	prim.c prim_prog.c prim_let.c prim_compar.c prim_math.c
OBJS = $(SRCS:.c=.o)
OUT = libaselsp.a

CC = @CC@
RANLIB = @RANLIB@
CFLAGS = @CFLAGS@ -I@abs_top_builddir@/..
LDFLAGS = @LDFLAGS@
LIBS = @LIBS@

all: $(OBJS)
	ar cr $(OUT) $(OBJS)
	ranlib $(OUT)

clean:
	rm -rf $(OBJS) $(OUT) *.o

.SUFFIXES: .c .o
.c.o:
	$(CC) $(CFLAGS) -c $<
