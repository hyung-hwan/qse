SRCS = sce.c 
#	memory.c object.c symbol.c class.c array.c \
#	dict.c misc.c context.c name.c token.c parser.c bootstrp.c \
#	bytecode.c interp.c
OBJS = $(SRCS:.c=.o)
OUT = libxpawk.a

CC = @CC@
RANLIB = @RANLIB@
CFLAGS = @CFLAGS@ -I@abs_top_builddir@
LDFLAGS = @LDFLAGS@
LIBS = @LIBS@

all: $(OBJS)
	ar cr $(OUT) $(OBJS)
	ranlib $(OUT)

clean:
	rm -rf $(OBJS) $(OUT) *.o

.SUFFIXES: .c .o
.c.o:
	$(CC) $(CFLAGS) -c $<
